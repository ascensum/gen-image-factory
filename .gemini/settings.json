{
  "hooks": {
    "SessionStart": [{
      "type": "command",
      "command": ".claude/hooks/scripts/session_start.py",
      "name": "ai-memory-session-start",
      "timeout": 5000
    }],
    "AfterTool": [{
      "matcher": "Write|Edit",
      "type": "command",
      "command": ".claude/hooks/scripts/post_tool_capture.py",
      "name": "ai-memory-post-tool"
    }],
    "PreCompress": [{
      "type": "command",
      "command": ".claude/hooks/scripts/pre_compact_save.py",
      "name": "ai-memory-pre-compact"
    }]
  },
  "commands": {
    "search-memory": {
      "description": "Search memory system semantically across all collections",
      "script": "python3 /Users/vjatseslavjertsalov/.ai-memory/scripts/memory/search_cli.py"
    },
    "memory-status": {
      "description": "Check AI memory system health and statistics",
      "script": "python3 /Users/vjatseslavjertsalov/.ai-memory/scripts/memory/memory_status.py"
    }
  },
  "env": {
    "AI_MEMORY_PROJECT_ID": "gen-image-factory",
    "AI_MEMORY_INSTALL_DIR": "/Users/vjatseslavjertsalov/.ai-memory",
    "QDRANT_HOST": "localhost",
    "QDRANT_PORT": "26350",
    "EMBEDDING_HOST": "localhost",
    "EMBEDDING_PORT": "28080"
  }
}
